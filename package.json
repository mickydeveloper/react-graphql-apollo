  },
  "description": "edchandover",
  "main": "./dist/bundle.js",
  "scripts": {
    "build": "SET NODE_ENV=local&& webpack --config configs/webpack/webpack.local.js --display-error-details",
    "build:prod": "SET NODE_ENV=production&& webpack --config configs/webpack/webpack.prod.js --display-error-details",
    "start": "SET NODE_ENV=local&& npm run build && concurrently --kill-others \"npm run api\" \"npm run wds\"",
    "start:prod": "SET NODE_ENV=production&&npm run build:prod && concurrently --kill-others \"npm run api\" \"npm run wds\"",
    "api": "node api/server.js",
    "wds": "webpack-dev-server --config configs/webpack/webpack.local.js",
    "test": "SET NODE_ENV=local&& jest",
    "test:format:json": "SET NODE_ENV=local&&jest --json",
    "test:update": "jest -u",
    "test:watch": "SET NODE_ENV=local&& jest --watch",
    "e2e": "ts-node --project test/tsconfig.test.json test/e2e/e2e-local.ts",
    "gen": "node api/data-generators/generateTodos.js"
  },
  "jest": {
    "setupFiles": [
      "./test/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "jsx",
      "js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "cobertura"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "license": "ISC"
